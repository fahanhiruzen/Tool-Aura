{
  "name": "tool-aura",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "main": "esbuild main/code.ts --bundle --outfile=dist/code.js --target=es6 --platform=neutral --external:figma --global-name=__plugin__",
    "dev": "vite",
    "ui": "vite build",
    "watch-plugin": "concurrently -n main,ui \"npm run main -- --watch\" \"npm run ui -- --watch\"",
    "build": "concurrently -n main,ui \"npm run main\" \"npm run ui\"",
    "watch": "node build-plugin.mjs --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:plugin": "node build-plugin.mjs",
    "watch:plugin": "node build-plugin.mjs --watch"
  },
  "dependencies": {
    "@figma/eslint-plugin-figma-plugins": "^1.0.0",
